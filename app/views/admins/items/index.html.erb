	<table class="table table-hover table-inverse">
			<thead>
				<tr>
					<th>商品ID</th>
					<th>商品名</th>
					<th>税抜き価格</th>
					<th>ジャンル</th>
					<th>ステータス</th>
				</tr>
			</thead>

			<tbody>
				<% @items.each do |item| %>
				<tr>
					<th><%=item.id%></th>
					<td><%= item.name %></td>
					<td><%= item.price %></td>
					<td><%= item.genre_id %></td>
					<td><%= item.is_sale_status %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
