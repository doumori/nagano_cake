	<div class="table">
		<table class="table table-hover table-inverse">
				<thead>
					<tr>
						<th>商品ID</th>
						<th>商品名</th>
						<th>税抜き価格</th>
						<th>ジャンル</th>
						<th>ステータス</th>
					</tr>
				</thead>

				<tbody>
					<% @items.each do |item| %>
					<tr>
						<th><%=item.id%></th>
					<td>
						<%= link_to item.name, admins_item_path(item.id) %>
						</td>
						<td><%= item.price %></td>
						<td><%= %></td>
						<td><%= item.is_sale_status  == false ? "販売停止中" : "販売中"%></td>

					</tr>
					<% end %>
				</tbody>
			</table>
		</div>
